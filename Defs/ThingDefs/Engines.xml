<?xml version="1.0" encoding="UTF-8"?>

<Defs>

<ThingDef ParentName="ResourceBase">
    <defName>LP2_smallEngine</defName>
    <label>small engine</label>
    <description>A small mono-cylinder engine.\n\nIt is used to build low power vehicles.</description>
    <graphicData>
      <texPath>Things/Item/Resource/ComponentSpacer</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>5</stackLimit>
    <healthAffectsPrice>true</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <MarketValue>400</MarketValue>
      <Mass>15</Mass>
      <Flammability>0</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>

    <costList>
      <Steel>20</Steel>
      <ComponentIndustrial>10</ComponentIndustrial>
    </costList>

</ThingDef>

</Defs>